{% extends form_errors %}

{% block fos_user_header %}{{ 'layout.register'|trans }}:{% endblock %}

{% block fos_user_content %}
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" id="fos_user_registration_register">
    {{ form_widget(form) }}
    <input type="submit" value="{{ 'registration.submit'|trans }} &rarr;" class="dropsubmitbtn" />
</form>
{% endblock fos_user_content %}
