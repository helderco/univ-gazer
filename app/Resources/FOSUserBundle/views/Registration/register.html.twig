{% extends "FOSUserBundle::form_errors.html.twig" %}

{% block fos_user_header %}{{ 'layout.register'|trans({}, 'FOSUserBundle') }}:{% endblock %}

{% block fos_user_content %}
<form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" id="fos_user_registration_register">
    {{ form_widget(form) }}
    <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }} &rarr;" class="dropsubmitbtn" />
</form>
{% endblock fos_user_content %}
